DROP TABLE IF EXISTS memes_saved;
CREATE TABLE IF NOT EXISTS memes_saved (
    uuid uuid,
	id text,
	sessionid uuid,
	name text,
	url text,
	toptext text,
	bottomtext text,
	memename text,
	width int,
	height int,
	box_count smallint,
	image_source text,
  	PRIMARY KEY (uuid)
);
CREATE INDEX memes_saved_id_idx ON memes_saved (id);

DROP TABLE IF EXISTS memes_likes;
CREATE TABLE IF NOT EXISTS memes_likes (
	uuid uuid,
	likes counter,
  	PRIMARY KEY (uuid)
);


